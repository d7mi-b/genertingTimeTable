<style>
    body {
        font-family: Arial, Helvetica, sans-serif;
        direction: rtl;
        font-size: 14px;
    }

    .container {
        width: 90%;
        margin: 0 auto;
    }

    /* header page */
    .header {
        text-align: center;
        padding: 10px;
        border-bottom: 5px solid #000;
    }

    .header .header-content {
        display: flex;
        justify-content: space-between;
    }

    .bold {
        font-weight: 600;
    }

    .header-content .logo svg {
        width: 100px;
    }

    /* content of page */
    .container .content h1 {
        margin: 55px auto;
        text-align: center;
        text-decoration: underline;
    }

    .content table,th,td {
        font-size: 10px;
        border: 1px solid black;
        border-collapse: collapse;
    } 

    .content{
        display: flex;
        flex-direction: column;
        text-align: center;
    }
    .content th{
        width: 10%;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .row{
        display: flex;
        width: 100%;
    }
    .row div{
        border-left: 1px solid black;
        border-right: 1px solid black;
        text-align: center;
        padding: 3px;
    }

    .user-data {
        text-decoration: underline;
    }

    .content footer {
        font-size: 22px;
        font-weight: 600;
        text-align: center;
    }

    .footer {
        display: flex;
        justify-content: space-between;
    }
</style>
<html>
    <body>
        <div class="container">
            <header class="header">
                <section class="header-content">
                    <div class="header-info">
                        <p class="bold">جامعة حضرموت</p>
                        <p class="bold">كلية الهندسة والبترول</p>
                        <p class="bold">قسم {{Department_Name}}</p>
                    </div>
                    <div class="logo">
                        <svg id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 222 224"><image width="222" height="224" xlink:href="data:image/jpeg;base64,"/></svg>
                    </div>
                    <div class="date">
                        <p class="bold">جدول المحاضرات الدراسية للفصل الدراسي {{semester}}</p>
                        <p class="bold">للعام الجامعي {{year}}</p>

                        <time class="bold" datetime="2023-06-07">التاريخ: {{date}}</time>
                    </div>
                </section>
            </header>

            <artical class="content">
                <h1>{{ title }}</h1>
                <table>   

                        <tr class="row">
                            <th><h3>الأحد</h3></th>
                            <td class="row">
                            {{#each scheduel_1 as |sun|}}
                            <div>
                                <h3>{{ sun.Subject_Name }}</h3>
                                <p>{{ sun.Lecturer_Name }}</p>
                                <p>{{ sun.Group_ }}</p>
                                <p>{{ sun.Hall_Name }}</p>
                                <p>{{ sun.End_Time }}-{{sun.Start_Time}}</p>
                            </div>
                            
                            {{/each}}
                            </td>
                        </tr>
                      
                        <tr class="row">
                            <th><h3>الإثنين</h3></th>
                            <td class="row">
                            {{#each scheduel_2 as |mon|}}
                            <div>
                                <h3>{{ mon.Subject_Name }}</h3>
                                <p>{{ mon.Lecturer_Name }}</p>
                                <p>{{ mon.Group_ }}</p>
                                <p>{{ mon.Hall_Name }}</p>
                                <p>{{ mon.End_Time }}-{{mon.Start_Time}}</p>
                            </div>
                            {{/each}}
                            </td>

                        </tr>
                      
                        <tr class="row">
                            <th><h3>الثلاثاء</h3></th>
                            <td class="row">
                            {{#each scheduel_3 as |tue|}}
                            <div>
                                <h3>{{ tue.Subject_Name }}</h3>
                                <p>{{ tue.Lecturer_Name }}</p>
                                <p>{{ tue.Group_ }}</p>
                                <p>{{ tue.Hall_Name }}</p>
                                 <p>{{ tue.End_Time }}-{{tue.Start_Time}}</p>
                            </div>
                             {{/each}}
                            </td>
                        </tr>
                       
                        <tr class="row">
                            <th><h3>الأربعاء</h3></th>
                            <td class="row">
                            {{#each scheduel_4 as |wed|}}
                            <div>
                                <h3>{{ wed.Subject_Name }}</h3>
                                <p>{{ wed.Lecturer_Name }}</p>
                                <p>{{ wed.Group_ }}</p>
                                <p>{{ wed.Hall_Name }}</p>
                                <p>{{ wed.End_Time }}-{{wed.Start_Time}}</p>
                            </div>
                            {{/each}}
                            </td>
                        </tr>
                            
                           
                        <tr class="row">
                            <th><h3>الخميس</h3></th>
                            <td class="row">
                            {{#each scheduel_5 as |thu|}}
                            <div>
                                <h3>{{ thu.Subject_Name }}</h3>
                                <p>{{ thu.Lecturer_Name }}</p>
                                <p>{{ thu.Group_ }}</p>
                                <p>{{ thu.Hall_Name }}</p>
                                <p>{{ thu.End_Time }}-{{thu.Start_Time}}</p>
                            </div>
                            {{/each}}
                            </td>
                        </tr>
                           
                     </tbody> 
                     
                    </table>
            </artical>

            

            <footer class="footer">
                <p>رئيس القسم</p>
                <p>النائب الأكاديمي</p>
                <p>عميد الكلية</p>
            </footer>
        </div>
        
    </body>

</html>
